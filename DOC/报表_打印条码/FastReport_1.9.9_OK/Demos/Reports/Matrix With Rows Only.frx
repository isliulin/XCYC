          <TableCell Name="Cell2" Border.Lines="All" Border.Color="White" Fill="Glass" Fill.Color="YellowGreen" Fill.Blend="0.29" Fill.Hatch="false" Text="Revenue" HorzAlign="Center" VertAlign="Center" Font="Tahoma, 8pt, style=Bold"/>
