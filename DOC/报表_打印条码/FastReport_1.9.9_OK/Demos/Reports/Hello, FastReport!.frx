<?xml version="1.0" encoding="utf-8"?>
<Report TextQuality="Regular" ReportInfo.Description="Demonstrates how to add simple dialog form to the report. To do this:&#13;&#10;&#13;&#10;- press the &quot;New Dialog&quot; button on a toolbar;&#13;&#10;&#13;&#10;- this will create an empty dialog form with two default buttons. Put some controls on it and set its properties as you want.&#13;&#10;&#13;&#10;When you run a report, you will see the dialog. If you close it by &quot;OK&quot; button, the report will be generated.&#13;&#10;&#13;&#10;Note: you may add several dialogs." ReportInfo.Created="01/18/2008 03:08:14" ReportInfo.Modified="01/21/2008 18:32:04">
  <Dictionary/>
  <ReportPage Name="Page1">
    <ReportTitleBand Name="ReportTitle1" Width="718.2" Height="37.8">
      <TextObject Name="Text1" Top="9.45" Width="245.7" Height="18.9" Text="The report goes here..."/>
    </ReportTitleBand>
  </ReportPage>
  <DialogPage Name="Form1" Width="212" Height="172" Text="Hello, FastReport.Net!">
    <ButtonControl Name="Button1" Left="24" Top="104" Font="Tahoma, 8pt" Text="OK" DialogResult="OK"/>
    <ButtonControl Name="Button2" Left="104" Top="104" Font="Tahoma, 8pt" TabIndex="1" Text="Cancel" DialogResult="Cancel"/>
    <LabelControl Name="Label1" Left="16" Top="16" Width="172" Height="55" Font="Tahoma, 8pt" TabIndex="2" Text="Press OK button to run a report; press Cancel to terminate it." AutoSize="false" TextAlign="MiddleCenter"/>
  </DialogPage>
</Report>
