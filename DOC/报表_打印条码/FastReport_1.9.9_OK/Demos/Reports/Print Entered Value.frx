<?xml version="1.0" encoding="utf-8"?>
<Report TextQuality="Regular" ReportInfo.Description="Demonstrates how to add simple dialog form to the report. To do this:&#13;&#10;&#13;&#10;- press the &quot;New Dialog&quot; button on a toolbar;&#13;&#10;&#13;&#10;- this will create an empty dialog form with two default buttons. Put some controls on it and set its properties as you want.&#13;&#10;&#13;&#10;When you run a report, you will see the dialog. If you close it by &quot;OK&quot; button, the report will be generated.&#13;&#10;&#13;&#10;Note: you may add several dialogs." ReportInfo.Created="01/18/2008 03:08:14" ReportInfo.Modified="10/07/2008 03:51:33" ReportInfo.CreatorVersion="1.0.0.0">
  <Dictionary/>
  <ReportPage Name="Page1">
    <ReportTitleBand Name="ReportTitle1" Width="718.2" Height="37.8">
      <TextObject Name="Text1" Top="9.45" Width="463.05" Height="18.9" Text="You have entered: [TextBox1.Text]"/>
    </ReportTitleBand>
  </ReportPage>
  <DialogPage Name="Form1" Width="212" Height="172">
    <ButtonControl Name="Button1" Left="24" Top="104" Font="Tahoma, 8pt" TabIndex="1" Text="OK" DialogResult="OK"/>
    <ButtonControl Name="Button2" Left="104" Top="104" Font="Tahoma, 8pt" TabIndex="2" Text="Cancel" DialogResult="Cancel"/>
    <LabelControl Name="Label1" Left="8" Top="12" Width="79" Height="13" Font="Tahoma, 8pt" TabIndex="3" Text="Enter the text:"/>
    <TextBoxControl Name="TextBox1" Left="8" Top="32" Width="188" Font="Tahoma, 8pt" TabIndex="0" Text="Hello!"/>
  </DialogPage>
</Report>
